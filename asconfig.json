{
  "targets": {
    "main": {
      "outFile": "build/main.wasm",
      "textFile": "build/main.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": false,
      "exportStart": true,
      "exportTable": true,
      "exportRuntime": true,
      "enable": ["reference-types", "gc"]
    }
  },
  "options": {
    "bindings": "esm"
  }
}
