{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": []
  },
  "tstl": {
    "noHeader": true,
    "luaTarget": "JIT",
    "luaBundleEntry": "src/main.ts",
    "luaBundle": "dist/main.lua"
  }
}
